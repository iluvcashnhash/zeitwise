# Application
ENVIRONMENT=development
DEBUG=True
SECRET_KEY=your-secret-key-here

# Database
POSTGRES_SERVER=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=zeitwise
DATABASE_URI=postgresql://postgres:postgres@postgres:5432/zeitwise

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=redispass

# Qdrant
QDRANT_URL=http://qdrant:6333

# Supabase
SUPABASE_URL=http://localhost:54321
SUPABASE_KEY=your-anon-key
SUPABASE_JWT_SECRET=your-super-secret-jwt-token
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_AUTH_COOKIE_NAME=sb-access-token
SUPABASE_REFRESH_COOKIE_NAME=sb-refresh-token
SUPABASE_TOKEN_EXPIRY=3600
SUPABASE_REFRESH_TOKEN_EXPIRY=604800

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# xAI Grok
GROK_API_KEY=your-grok-api-key

# Logging
LOG_LEVEL=INFO

# CORS (comma-separated list of origins)
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:19006

# PGAdmin
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin
